{
  "name": "fastn",
  "displayName": "fastn",
  "description": "This extension provides syntax highlighting support for the `fastn` language in Visual Studio  Code. Enjoy enhanced readability of your FTD code with vibrant and accurate syntax highlighting.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harshdoesdev/fastn-syntax.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ftd",
        "aliases": [
          "FifthTry Document",
          "ftd"
        ],
        "extensions": [
          "ftd"
        ],
        "configuration": "./src/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ftd",
        "scopeName": "source.ftd",
        "path": "./src/syntaxes/ftd.tmLanguage.json"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/harshdoesdev/fastn-syntax/issues"
  },
  "homepage": "https://github.com/harshdoesdev/fastn-syntax#readme",
  "scripts": {
    "package": "vsce package"
  },  
  "devDependencies": {}
}
